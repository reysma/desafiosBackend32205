
    <h1>Ecommerce</h1>
    
<table class="table">
  <thead>
    <tr>
      
      <th scope="col">title</th>
      <th scope="col">description</th>
      <th scope="col">Imagen</th>
      <th scope="col">precio</th>
      <th scope="col">stock</th>
    </tr>
  </thead>
</table>


<tr>
    <th scope ="row"> {{ this.id }}</th> 
    <td> {{ this.title}}</td>
    <td> {{ this.description }}</td>
    <td><img src="{{ this.thumbnail }}"></td>
    <td> {{ this.price }}</td>
    <td> {{ this.code }}</td>
</tr>
{{/each}} 