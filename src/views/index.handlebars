<h1>Ecommerce</h1>
<div>  
<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Model</th>
      <th>Id</th>
      <th>Imagen</th>
      <th>Precio</th>
      <th>Code</th>
      <th>Stock</th>
    </tr>
  </thead>
 <tbody> 
    {{#each products}}
    <tr>
        <td> 
          <b>{{ this.title }}</b>
          </td>
          <td> 
          <b>{{ this.model }}</b>
          </td>
            <td> 
          <b>{{ this.id }}</b>
          </td>
          <td>
            <img wdth="50"src="{{ this.image }}">
          </td>
          <td> 
            <p>{{ this.price }}</p>
          </td>
          <td> 
            <p>{{ this.code }}</p>
          </td>
          <td> 
            <p>{{ this.stock }}</p>
          </td>
    </tr>
    {{/each}} 
</tbody>
</table>
<style>
    table,
    th,
    tr {
        border: 1px solid;
    }
    .title {
        text-align: center;
    }
    .product-list {
        width: 100%;
    }
</style>